  <script>
    // Get all FAQ pages
    const faqPages = document.querySelectorAll('.faq-page');

    // Add click event listener to each FAQ page
    faqPages.forEach((faqPage) => {
      faqPage.addEventListener('click', () => {
        // Toggle the visibility of the corresponding FAQ body
        const faqBody = faqPage.nextElementSibling;
        faqBody.style.display = (faqBody.style.display === 'block') ? 'none' : 'block';
      });
    });
